<%= form_for(@rt_other, default_form_options) do |f| %>
<div class="form form-electronic">
  <%= render(partial: 'shared/errors', locals: {model_name: @rt_other})%>

  <p class="notes">
   Los campos marcados con <em class="asterisco">*</em> son obligatorios.
  </p>

<fieldset>
    <div class="row">
      <div class="form-group">
        <div class="col-xs-12 col-sm-12 col-md-12">
          <label class="labelUp"><%= Rt::Other.human_attribute_name(:description) %> <span class="asterisco">*</span></label>
          <%= f.text_field :description, { class: 'big' } %>
        </div>
      </div>
    </div>
 </fieldset>

</div>

<div class="button-group text-right">
  <%= f.button t('application.submit'), :class => 'event-link big' %>
</div>
<% end %>
