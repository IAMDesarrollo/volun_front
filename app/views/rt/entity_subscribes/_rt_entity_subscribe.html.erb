<tr id="<%= dom_id(rt_entity_subscribe) %>" class="table-row-transform" >

  <% project.main_attributes.each do |attr_name| %>
  <td class="table-cell-group"><%= show_attr(rt_entity_subscribe.public_send attr_name) %></td>
  <% end %>

  <td class="table-cell-group">
    <%= link_to_show(rt_entity_subscribe) %>
    <%= link_to_edit(rt_entity_subscribe) %>
<% if rt_entity_subscribe.try(:archived?) %>
    <%= link_to_recover(rt_entity_subscribe) %>
<% else %>
    <%= link_to_destroy(rt_entity_subscribe) %>
<% end %>
  </td>
</tr>
