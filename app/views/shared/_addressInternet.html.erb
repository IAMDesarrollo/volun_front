    <fieldset>
        <div class="row">
            <div class="col-md-9">
                <div class="form-group">
                     <label class="labelUp"><%= Rt::EntitySubscribe.human_attribute_name(:road_type) %> <span class="asterisco">*</span></label>
                <%= f.select :road_type, road_types_select_options, {include_blank: "", selected: ""},  class: 'small'  %>
                </div>
                <div class="form-group">
                    <label class="labelUp"><%= Rt::EntitySubscribe.human_attribute_name(:road_name) %> <span class="asterisco">*</span></label>
                <%= f.text_field :road_name, { class: 'big' } %>
                </div>
                <div class="form-group">
                    <label class="labelUp"><%= Rt::EntitySubscribe.human_attribute_name(:number_type) %><span class="asterisco">*</span></label>
                <%= f.select :number_type, number_types_select_options, {include_blank: "", selected: ""},  class: 'small' %>
                </div>
                <div class="form-group">
                    <label class="labelUp"><%= Rt::EntitySubscribe.human_attribute_name(:road_number) %> <span class="asterisco">*</span></label>
                <%= f.text_field :road_number, { class: 'small' }  %>
                </div>
                <div class="form-group">
                      <label class="labelUp"><%= Rt::EntitySubscribe.human_attribute_name(:postal_code) %><span class="asterisco">*</span></label>
              <%= f.text_field :postal_code, { class: 'small' } %>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-9">
               <div class="form-group">
                    <label class="labelUp"><%= Rt::EntitySubscribe.human_attribute_name(:province) %> <span class="asterisco">*</span></label>
              <%= f.select :province, provinces_select_options, {include_blank: "", selected: ""},  class: 'medium' %>
                </div>
                <div class="form-group">
                     <label class="labelUp"><%= Rt::EntitySubscribe.human_attribute_name(:town) %><span class="asterisco">*</span></label>
              <%= f.text_field :town, { class: 'medium' } %>
                </div>


            </div>
        </div>
    </fieldset>

