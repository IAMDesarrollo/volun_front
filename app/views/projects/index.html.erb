      <% if current_user.loggable_type=="Entity" %>
<div class="container-fluid">


  <div class="row">
      <div class="col-md-12">
          <ul class="pestaniasActividadProyecto">

            <li><%= link_to 'Tus proyectos', projects_path(q: {entity_id_eq: current_user.loggable_id}, entity_id: current_user.loggable_id), remote: true %></li>
             <li><%= link_to 'Tus Actividades', entity_activities_path(q: {entity_id_eq: current_user.loggable_id}, entity_id: current_user.loggable_id), remote: true %></li>
</ul>
      </div>
  </div>



