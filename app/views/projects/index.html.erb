    <section class="container-fluid projectW">

 <div class="row">
     <div class="col-md-12">
         <% if current_user.loggable_type=="Entity" %>
          <% else %>
         <h2 class="titlePage">Tus proyectos asignados</h2>
         <% end %>
         <h4 class="projectNumber"><%= Project.actives.count %> proyectos</h4>
     </div>
 </div>


    <ul class="row">
        <div id = "projects">
          <% @projects_actives.each_slice(3) do |group| %>
              <%= render partial: 'project', collection: group %>
          <% end %> 
        </div>
    </ul>

    <ul class="row">
        <div id = "activities">
        <%= render partial: 'activities/activity', collection: @activities %>
        </div>
    </ul>
</section>
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <% if @projects_actives.current_page != @projects_actives.total_pages %>
<p id="view-more">
   <%= link_to('Ver Más', url_for(page: @projects_actives.current_page + 1), {:remote => true}) %>
</p>
<% end%>
        </div>
    </div>
</div>